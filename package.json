    "build": "npm run _build && npm run _pkg"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@zwfthcks/zwift-log-monitor": "github:zwfthcks/zwift-log-monitor",
    "minimist": "^1.2.8"
  },
  "bin": ".build/index.js"
}
