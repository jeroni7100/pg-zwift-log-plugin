{
  "name": "pg-zwift-log-plugin",
  "version": "1.0.0",
  "description": "Send Zwift log data to Pedal Games",
  "module": "emitter/index.mjs",
  "scripts": {
    "start": "node emitter/index.mjs",
    "test": "echo \"Error: no test specified\" && exit 1",
